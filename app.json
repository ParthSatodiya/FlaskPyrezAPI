{
    "addons": [
        { "plan": "scheduler:standard" },
        { "plan": "heroku-postgresql:hobby-dev", "as": "PALADINS_DB" },
        { "plan": "heroku-postgresql:hobby-dev", "as": "SMITE_DB" },
        { "plan": "heroku-postgresql:hobby-dev", "as": "DISCORD_DB" },
        { "plan": "heroku-postgresql:hobby-dev", "options": { "version": "11" }, "as": "DATABASE" }
    ],
    "buildpacks": [
        { "url": "https://github.com/heroku/heroku-buildpack-python" },
        { "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git" }
    ],
    "description": "FlaskPyrezAPI is an open-source endpoint for Twitch commands that provides datas about players, matches and others stats of Paladins game.",
    "env": {
        "DEBUG": {
            "description": "",
            "required": "true",
            "value": "False"
        },
        "DISCORD_BOT_TOKEN": {
            "description": "Used by Discord.py connecting to Discord. This is the botâ€™s token that you receive from Discord.",
            "required": "true",
            "value": ""
        },
        "FB_ACCESS_TOKEN": {
            "description": "The ACCESS_TOKEN used to post messages on FB messenger",
            "required": "true",
            "value": "CHANGE_ME"
        },
        "FB_VERIFY_TOKEN": {
            "description": "The VERIFY_TOKEN used by FB messenger to verify that this bot is yours.",
            "generator": "secret"
        },
        "ON_HEROKU": {
            "description": "",
            "value": "True"
        },
        "PYREZ_AUTH_ID": {
            "description": "Used for authentication. This is the authentication key that you receive from Hi-Rez Studios.",
            "required": "true",
            "value": ""
        },
        "PYREZ_DEV_ID": {
            "description": "Used for authentication. This is the developer ID that you receive from Hi-Rez Studios.",
            "required": "true",
            "value": ""
        },
        "SECRET_KEY": {
            "description": "A secret key, automaticaly generated by heroku, for verifying the integrity of signed cookies and could be used to hash some passwords.",
            "generator": "secret"
        },
        "APP_SECRET_TOKEN": {
            "description": "",
            "generator": "secret"
        }
    },
    "environments": {
        "test": {
            "scripts": {
            }
        }
    },
    "image": "heroku/python",
    "keywords": [
        "flask", "flask-api", "flask-backend", "pyrez", "hirez", "hi-rez", "paladins",
        "paladins-api", "api-server", "twitch", "nightbot", "endpoint", "endpoints",
        "open-source", "heroku", "python", "bootstrap4", "bootstrap", "jquery", "javascript",
        "sqlalchemy", "postgresql"
    ],
    "logo": "https://raw.githubusercontent.com/luissilva1044894/FlaskPyrezAPI/master/static/img/pyrez_server.png",
    "name": "FlaskPyrezAPI",
    "repository": "https://github.com/luissilva1044894/flaskpyrezapi",
    "success_url": "/index",
    "website": "http://nonsocial.herokuapp.com"
}